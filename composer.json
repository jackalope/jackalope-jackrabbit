{
    "name": "jackalope/jackalope-jackrabbit",
    "type": "library",
    "description": "Jackalope Transport library",
    "keywords": ["transport implementation", "jackrabbit"],
    "homepage": "http://jackalope.github.com",
    "license": [
        "MIT",
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Jackalope Community",
            "homepage": "https://github.com/jackalope/jackalope-jackrabbit/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "ext-libxml":"*",
        "ext-curl":"*",
        "phpcr/phpcr": "~2.1.0-RC1",
        "phpcr/phpcr-utils": "~1.0.0-RC1",
        "jackalope/jackalope": "1.0.0-RC1"
    },
    "provide": {
        "jackalope/jackalope-transport": "1.0.0-RC1"
    },
    "require-dev": {
        "phpcr/phpcr-api-tests": "dev-master",
        "symfony/console": "~2.0"
    },
    "autoload": {
        "psr-0": { "Jackalope\\": "src/" }
    },
    "bin": ["bin/jackalope", "bin/jackrabbit.sh"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
