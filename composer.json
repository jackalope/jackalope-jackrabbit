{
    "name": "jackalope/jackalope-jackrabbit",
    "type": "library",
    "description": "Jackalope Transport library",
    "keywords": ["transport implementation", "jackrabbit"],
    "homepage": "http://jackalope.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jackalope Community",
            "homepage": "https://github.com/jackalope/jackalope-jackrabbit/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "ext-libxml":"*",
        "ext-curl":"*",
        "phpcr/phpcr": "2.1.0-beta8",
        "phpcr/phpcr-utils": "1.0-beta4",
        "jackalope/jackalope": "1.0.*"
    },
    "provide": {
        "jackalope/jackalope-transport": "1.0-beta1"
    },
    "require-dev": {
        "phpcr/phpcr-api-tests": "dev-master",
        "symfony/console": ">=2.0.0,<2.2.0-dev"
    },
    "autoload": {
        "psr-0": { "Jackalope\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
