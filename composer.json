{
    "name": "jackalope/jackalope-jackrabbit",
    "type": "library",
    "description": "Jackalope Transport library for Jackrabbit",
    "keywords": ["transport implementation", "jackrabbit", "phpcr"],
    "homepage": "http://jackalope.github.com",
    "license": [
        "MIT",
        "Apache-2.0"
    ],
    "authors": [
        {
            "name": "Jackalope Community",
            "homepage": "https://github.com/jackalope/jackalope-jackrabbit/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "ext-xml":"*",
        "ext-curl":"*",
        "phpcr/phpcr": "~2.1.2",
        "phpcr/phpcr-utils": "^1.2.9",
        "jackalope/jackalope": "~1.3.0"
    },
    "provide": {
        "jackalope/jackalope-transport": "1.3.0"
    },
    "require-dev": {
        "psr/log": "~1.0",
        "phpcr/phpcr-api-tests": "dev-opv_initialize",
        "symfony/console": "~2.0"
    },
    "autoload": {
        "psr-0": { "Jackalope\\": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Jackalope\\": "tests/" }
    },
    "bin": ["bin/jackalope", "bin/jackrabbit.sh"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:danrot/phpcr-api-tests.git"
        }
    ]
}
